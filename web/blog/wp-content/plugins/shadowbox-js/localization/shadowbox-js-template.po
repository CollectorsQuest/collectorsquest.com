#: inc/options-page.php:515
#: inc/options-page.php:528
#: inc/options-page.php:541
msgid "Default is true."
msgstr ""

#: inc/options-page.php:490
msgid "FLV Links"
msgstr ""

#: inc/options-page.php:499
msgid "Default is false.  To enable this option you must first <a href=\"#enableFlv\">enable FLV support</a>."
msgstr ""

#: inc/options-page.php:501
msgid "Default is false."
msgstr ""

#: inc/options-page.php:507
msgid "Movie Links"
msgstr ""

#: inc/options-page.php:520
