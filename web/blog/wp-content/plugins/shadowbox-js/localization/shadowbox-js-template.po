#: inc/options-page.php:533
msgid "YouTube and Google Video Links"
msgstr ""

#: inc/options-page.php:545
msgid "Sizes"
msgstr ""

#: inc/options-page.php:549
msgid "Generic Video Width"
msgstr ""

#: inc/options-page.php:554
msgid "The width of Shadowbox (in pixels) when displaying videos. Defaults to 640."
msgstr ""

#: inc/options-page.php:559
msgid "Generic Video Height"
msgstr ""

#: inc/options-page.php:564
msgid "The height of Shadowbox (in pixels) when when displaying videos. Defaults to 385."
msgstr ""

